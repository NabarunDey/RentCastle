<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="false" />

	<package name="viewProductByVendorPackage" namespace="/" extends="struts-default,json-default">

		<action name="viewProductByVendorFunction" class="viewProductByVendorAction" method="viewProductByVendorFunction">
			<result name="success" type="tiles">searchResult</result>
		</action>
		
		<action name="deleteProductFunction" class="viewProductByVendorAction" method="deleteProductFunction">
			<result name="success" type="tiles">searchResult</result>
		</action>
		
		<action name="editProductInputFunction" class="viewProductByVendorAction" method="editProductInputFunction">
			<result name="success" type="tiles">editProductInput</result>
		</action>
		
		<action name="editProductSubmitFunction" class="viewProductByVendorAction" method="editProductSubmitFunction">
			<result name="success" type="tiles">editRentOffersInput</result>
		</action>

	</package>

</struts>