<?xml version='1.0' encoding='UTF-8'?>  
    <!DOCTYPE hibernate-mapping PUBLIC  
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.databaseBeans.ServiceRequestDBBean" table="servicesrequest">
		<id name="servicerequestid">
			<generator class="increment"></generator>
		</id>
		<property name="servicetype"></property>
		<property name="customerusername"></property>
		<property name="customermobile"></property>
		<property name="address"></property>
		<property name="description"></property>
		<property name="city"></property>
		<property name="pin"></property>
		<property name="status"></property>
		<property name="serviceid"></property>
		<property name="providerrusername"></property>
		<property name="rentcastlecut"></property>
	</class>

</hibernate-mapping>  	