<?xml version='1.0' encoding='UTF-8'?>  
    <!DOCTYPE hibernate-mapping PUBLIC  
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.databaseBeans.PaymentsDBBean" table="payments">
		<id name="paymentid">
			<generator class="increment"></generator>
		</id>
		<property name="orderid"></property>
		<property name="fromusername"></property>
		<property name="tousername"></property>
		<property name="datetime"></property>
		<property name="securitymoney"></property>
		<property name="rentamount"></property>
		<property name="deliveryCharge"></property>
		<property name="paymentStatus"></property>
	</class>

</hibernate-mapping>  	