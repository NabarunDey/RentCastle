<?xml version='1.0' encoding='UTF-8'?>  
    <!DOCTYPE hibernate-mapping PUBLIC  
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.databaseBeans.ProductsDBBean" table="products">
		<id name="productid">
		<generator class="increment"></generator> 
		</id>
		<property name="productname"></property>
		<property name="producttype"></property>
		<property name="subproducttype"></property>
		<property name="username"></property>
		<property name="quantity"></property>
		<property name="images"></property>
		<property name="status"></property>
		<property name="securitymoney"></property>
		<property name="productstate"></property>
		<property name="productcity"></property>
		<property name="productlocation"></property>
		<property name="approvalStatus"></property>
	</class>

</hibernate-mapping>  	