<?xml version='1.0' encoding='UTF-8'?>  
    <!DOCTYPE hibernate-mapping PUBLIC  
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.databaseBeans.CurrentHoldingsDBBean" table="currentholdings">
		<id name="holdingid">
			<generator class="increment"></generator>
		</id>

		<property name="username"></property>
		<property name="orderid"></property>
		<property name="productid"></property>
		<property name="rentOfferid"></property>
		<property name="itemreceiveddate"></property>
		<property name="rentexpirydate"></property>
		<property name="status"></property>

	</class>

</hibernate-mapping>  	