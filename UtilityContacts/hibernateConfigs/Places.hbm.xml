<?xml version='1.0' encoding='UTF-8'?>  
    <!DOCTYPE hibernate-mapping PUBLIC  
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.databaseBeans.PlacesDBBean" table="places">
		<id name="placeid">
			<generator class="increment"></generator>
		</id>
		<property name="placename"></property>
		<property name="placetype"></property>
		<property name="username"></property>
		<property name="description"></property>
		<property name="profileImage"></property>
		<property name="imagesGalleryId"></property>
		<property name="facilitiesId"></property>
		<property name="status"></property>
		<property name="joiningFees"></property>
		<property name="city"></property>
		<property name="pin"></property>
		<property name="address"></property>
		<property name="approvalStatus"></property>
	</class>

</hibernate-mapping>  	