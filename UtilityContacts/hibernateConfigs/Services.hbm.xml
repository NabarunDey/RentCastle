<?xml version='1.0' encoding='UTF-8'?>  
    <!DOCTYPE hibernate-mapping PUBLIC  
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.databaseBeans.ServicesDBBean" table="services">
		<id name="serviceid">
			<generator class="increment"></generator>
		</id>
		<property name="username"></property>
		<property name="title"></property>
		<property name="mobile1"></property>
		<property name="mobile2"></property>
		<property name="address"></property>
		<property name="description"></property>
		<property name="city"></property>
		<property name="pin"></property>
		<property name="approvalStatus"></property>
		<property name="rentcastlecut"></property>
	</class>

</hibernate-mapping>  	